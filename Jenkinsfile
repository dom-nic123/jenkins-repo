pipeline {
    agent any
    stages {
        stage("Build Stage") {
            steps {
                echo "This is a step inside my Build stage build"
                echo "This is the 2nd step"
            }
        }
        stage("Test Stage") {
            steps {
                echo "This is a step inside my Test stage build"
                echo "This is the 2nd step"
            }
        }
        stage("Deploy Stage") {
            steps {
                echo "This is a step inside my Deploy stage build"
                echo "This is the 2nd step"
            }
        }
    }
}
